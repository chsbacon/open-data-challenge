import pandas as pd
import time
import numpy as np
weather=pd.read_csv('C:\\Users\\steph\\Documents\\My General Files\\Astraea Open Data Challenge\\open-data-challenge\\data\\weather\\precip_temp.csv', parse_dates=[0]).values
def oneHot(npa,maxVal):
    oh=np.zeros((npa.shape[0],maxVal+1))
    oh[np.arange(npa.shape[0]),npa]=1
    return oh
dayOfWeek=oneHot(np.array([i.dayofweek for i in weather[:,0].tolist()]),6) #Day of week, one-hot encoded. Monday is 0.
dayReduced=np.array([[i.dayofyear] for i in weather[:,0].tolist()])/366 #Day of year, from 0 to 1
percipitation=weather[:,1:2] #Percipitation
tempMaxMin=weather[:,2:4] #Maximum and minimum temperatures
